{
  "name": "pfcode-electron-installer",
  "version": "1.0.0",
  "description": "Install software using winget",
  "productName": "pfcode-electron-installer",
  "main": "main.js",
  "scripts": {
    "init": "electron .",
    "start": "npx electronmon .",
    "style": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "packager": "electron-packager ./ --platform=win32"
  },
  "repository": "https://github.com/Dragod/pfcode-electron-installer.git",
  "keywords": [
    "Electron",
    "winget",
    "installer",
    "pfcode"
  ],
  "author": "GitHub",
  "license": "MIT",
  "devDependencies": {
    "electron": "^21.2.3",
    "tailwindcss": "^3.2.4"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "copy-to-clipboard": "^3.3.3",
    "electron-packager": "^17.1.1",
    "qrcode": "^1.5.1",
    "toastify-js": "^1.12.0"
  }
}
